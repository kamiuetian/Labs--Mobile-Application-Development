<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqliteexampleapp">
    <uses-permission android:name="android.permission.READ_PHONE_STATE">

    </uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLiteExampleApp">


        <activity
            android:name=".servicesdemo.MediaPlayroom"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AsyncTaskLoader.Activitivty_AsyncLoader"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demoasynctask.AsynctaskActivity"
            android:exported="true" />

    <receiver android:name=".servicesdemo.MyReceiver"
        android:enabled="true"
        android:exported="true">
        <intent-filter>
            <action android:name="com.example.sqliteexample.servicedemo.CUSTOM_INTENT"></action>
        </intent-filter>
    </receiver>
        <service android:name=".servicesdemo.mediaplayerservice"/>
    </application>

</manifest>